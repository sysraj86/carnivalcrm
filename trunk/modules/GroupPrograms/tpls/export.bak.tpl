        <td class="tabDetailViewDL">Operator:</td>
        <td class="tabDetailViewDF">{OPERATOR}</td>
        <td class="tabDetailViewDL">&nbsp;</td>
        <td class="tabDetailViewDF">&nbsp;</td>
    </tr>
